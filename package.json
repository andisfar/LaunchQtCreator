{
	"name": "launchqtcreator",
	"displayName": "LaunchQtCreator",
	"description": "Launch QtCreator",
	"version": "0.0.1",
	"publisher": "OralShaneCurtis-6510",
	"author": {
		"name": "Oral Shane Curtis (AngryDane)",
		"email": "andisfar@hotmail.com",
		"url": "https://github.com/andisfar/"		
	},
	"repository": {"url": "https://github.com/andisfar/LaunchQtCreator"},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.launchqtcreator"		
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.launchqtcreator",
				"title": "Qt Creator"
			}
		],
		"configuration": [
			{
				"title": "launchqtcreator",
				"properties": {
					"launchqtcreator.qtCreatorPath": {
						"type": "string",
						"scope": "resource",
						"default": "",
						"description": "Enter the path of QtCreator for this machine"
					}
				}
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}